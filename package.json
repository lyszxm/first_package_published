{
  "name": "@01-monorepo-startup/first_package_published",
  "version": "2.0.3",
  "description": "first_package_published.",
  "author": "lsy",
  "license": "MIT",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/mjs/index.d.ts",
        "default": "./dist/mjs/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "types": "./dist/cjs/index.d.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/mjs/index.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "rarity",
    "nft",
    "web3",
    "nonfungibletoken",
    "crypto",
    "algorithm",
    "nftgo",
    "nodejs",
    "javascript"
  ],
  "homepage": "https://github.com/dmokel/GoRarity#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lyszxm/first_package_published"
  },
  "bugs": {
    "url": "https://github.com/lyszxm/first_package_published/issues"
  },
  "engines": {
    "node": ">=16.16.0"
  },
  "packageManager": "pnpm@8.15.4",
  "scripts": {
    "test": "node -v",
    "prepare": "husky install",
    "lint": "eslint ./ --max-warnings=0 --ext .ts",
    "lint-fix": "eslint ./ --max-warnings=0 --ext .ts --fix",
    "format": "prettier --config .prettierrc . --write",
    "format-watch": "onchange -- prettier . --write {{changed}}",
    "clean": "rimraf ./dist ./tsconfig.tsbuildinfo",
    "compile": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && fixup.sh",
    "build": "pnpm run clean && pnpm run compile",
    "output": "tsc -p tsconfig.json",
    "tag-push": "git push --follow-tags origin master",
    "tagPush": "git push origin --tags",
    "release:dry": "standard-version --release-as patch --dry-run",
    "release": "standard-version --release-as patch"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "jest": "29.4.0",
    "onchange": "^7.1.0",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "4.9.5",
    "standard-version": "^9.5.0",
    "rimraf": "^5.0.5"
  }
}
